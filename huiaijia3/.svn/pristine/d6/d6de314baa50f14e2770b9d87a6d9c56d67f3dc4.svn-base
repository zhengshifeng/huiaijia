package com.flf.enums;

/**
 * 专区类型枚举
 */
public enum areaType {

	BRAND_AREA("品牌专区", 1),
	RECOMMENDED_AREA("推荐专区", 2),
	PRODUCT_CATEGORY("商品类目", 3),
	COMMODITY_CATEGORIES("商品分类", 4);

	public String name;
	public int id;

	private areaType(String name, int id) {
		this.name = name;
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}
}
