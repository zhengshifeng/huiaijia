package com.flf.entity;

import com.base.entity.BaseEntity;

/**
 * 小区单元表
 * 
 * @author SevenWong<br>
 * @date 2016年9月20日下午2:23:02
 */
public class HajCommunityUnit extends BaseEntity {
	private static final long serialVersionUID = 7496514918672075737L;

	private Integer id;
	private String unit; // 小区单位地址
	private Integer sort; // 用于排序
	private Integer communityId; // haj_community_persion外键
	private Integer buildingId; // 楼栋表最后一级ID

	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public String getUnit() {
		return unit;
	}

	public void setUnit(String unit) {
		this.unit = unit;
	}

	public Integer getSort() {
		return sort;
	}

	public void setSort(Integer sort) {
		this.sort = sort;
	}

	public Integer getCommunityId() {
		return communityId;
	}

	public void setCommunityId(Integer communityId) {
		this.communityId = communityId;
	}

	public Integer getBuildingId() {
		return buildingId;
	}

	public void setBuildingId(Integer buildingId) {
		this.buildingId = buildingId;
	}
}
