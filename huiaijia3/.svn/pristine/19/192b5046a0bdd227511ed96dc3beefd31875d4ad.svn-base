package com.flf.controller.erp.entity;

/**
 * 采购订单Dto
 * Created by Administrator on 2017/11/21.
 */

import java.io.Serializable;
import java.math.BigDecimal;


public class ERPPurchaseOrderDto implements Serializable {
	private String  FOrgCode;    //组织代码 对应城市编码
	private String  FSupplierCode; //供应商代码 对应K3 clouds中的id
	private String  FDate;       //日期
	private String  FItemCode;   //料号
	private BigDecimal FQty;     //采购数量
	private BigDecimal FAmount;  //采购金额

	public String getFDate() {
		return FDate;
	}

	public void setFDate(String FDate) {
		this.FDate = FDate;
	}

	public String getFSupplierCode() {
		return FSupplierCode;
	}


	public void setFSupplierCode(String FSupplierCode) {
		this.FSupplierCode = FSupplierCode;
	}

	public void setFOrgCode(String FOrgCode) {
		this.FOrgCode = FOrgCode;
	}


	public void setFItemCode(String FItemCode) {
		this.FItemCode = FItemCode;
	}

	public void setFQty(BigDecimal fQty) {
		FQty = fQty;
	}

	public void setFAmount(BigDecimal famount) {
		FAmount = famount;
	}

	public String getFOrgCode() {
		return FOrgCode;
	}


	public String getFItemCode() {
		return FItemCode;
	}

	public BigDecimal getFQty() {
		return FQty;
	}

	public BigDecimal getFAmount() {
		return FAmount;
	}




}
