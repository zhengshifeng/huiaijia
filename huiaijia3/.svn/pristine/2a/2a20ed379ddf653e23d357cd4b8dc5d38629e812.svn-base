package com.flf.entity;import com.base.entity.BaseEntity;/** * <br> * <b>功能：</b>HajCategoryEntity<br> */public class HajCategory extends BaseEntity {	private Integer id;//	private String name;//	private Integer sort;//	private Page page;	public Integer getId() {		return this.id;	}	public void setId(Integer id) {		this.id = id;	}	public String getName() {		return this.name;	}	public void setName(String name) {		this.name = name;	}	public Integer getSort() {		return this.sort;	}	public void setSort(Integer sort) {		this.sort = sort;	}	public Page getPage() {		if (page == null) {			page = new Page();		}		return page;	}	public void setPage(Page page) {		this.page = page;	}}