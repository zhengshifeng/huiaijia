package com.flf.entity;

import com.base.entity.BaseEntity;

import java.math.BigDecimal;
import java.util.Date;
import java.util.List;
import java.util.Map;

/**
 * <br>
 * <b>功能：</b>HajActivityEntity<br>
 */
public class HajActivity extends BaseEntity {
	private static final long serialVersionUID = -5835820249906741638L;

	private Integer id;//
	private String activityName;// 促销活动名称
	private Integer status;// 是否启用（1是 0否）
	private BigDecimal discountAmount;// 活动优惠金额
	private String beginTime;// 开始时间
	private String endTime;// 结束时间
	private Date createTime;// 创建时间
	private Integer number; // 促销活动总数（份）
	private Integer useNumber; // 已使用的总数（份）
	private String residentials; // 小区
	private String operator; // 活动提交人
	private String describer;// 活动描述
	private String sbTypeName; // 大分类
	private String sTypeName; // 小分类
	private String commodityNo; // 商品名称
	private String commodityIds;
	private Integer activityType; // 活动类型(0: 限时抢购; 1: 1元购; 2: 首页banner活动; 3: 新品推荐; 4: 爆品热卖)
	private String img; // 活动图片地址
	private String link; // 活动链接

	private Page page;
	private List<Map<String, Object>> commodityList;

	public String getResidentials() {
		return residentials;
	}

	public void setResidentials(String residentials) {
		this.residentials = residentials;
	}

	public java.lang.Integer getNumber() {
		return number;
	}

	public void setNumber(java.lang.Integer number) {
		this.number = number;
	}

	public java.lang.Integer getUseNumber() {
		return useNumber;
	}

	public void setUseNumber(java.lang.Integer useNumber) {
		this.useNumber = useNumber;
	}

	public java.lang.Integer getId() {
		return this.id;
	}

	public void setId(java.lang.Integer id) {
		this.id = id;
	}

	public java.lang.String getActivityName() {
		return this.activityName;
	}

	public void setActivityName(java.lang.String activityName) {
		this.activityName = activityName;
	}

	public java.lang.Integer getStatus() {
		return this.status;
	}

	public void setStatus(java.lang.Integer status) {
		this.status = status;
	}

	public BigDecimal getDiscountAmount() {
		return this.discountAmount;
	}

	public void setDiscountAmount(BigDecimal discountAmount) {
		this.discountAmount = discountAmount;
	}

	public String getBeginTime() {
		return beginTime;
	}

	public void setBeginTime(String beginTime) {
		this.beginTime = beginTime;
	}

	public String getEndTime() {
		return endTime;
	}

	public void setEndTime(String endTime) {
		this.endTime = endTime;
	}

	public java.util.Date getCreateTime() {
		return this.createTime;
	}

	public void setCreateTime(java.util.Date createTime) {
		this.createTime = createTime;
	}

	public Page getPage() {
		return page;
	}

	public void setPage(Page page) {
		this.page = page;
	}

	public String getOperator() {
		return operator;
	}

	public void setOperator(String operator) {
		this.operator = operator;
	}

	public String getDescriber() {
		return describer;
	}

	public void setDescriber(String describer) {
		this.describer = describer;
	}

	public String getSbTypeName() {
		return sbTypeName;
	}

	public void setSbTypeName(String sbTypeName) {
		this.sbTypeName = sbTypeName;
	}

	public String getsTypeName() {
		return sTypeName;
	}

	public void setsTypeName(String sTypeName) {
		this.sTypeName = sTypeName;
	}

	public String getCommodityNo() {
		return commodityNo;
	}

	public void setCommodityNo(String commodityNo) {
		this.commodityNo = commodityNo;
	}

	public String getCommodityIds() {
		return commodityIds;
	}

	public void setCommodityIds(String commodityIds) {
		this.commodityIds = commodityIds;
	}

	public List<Map<String, Object>> getCommodityList() {
		return commodityList;
	}

	public void setCommodityList(List<Map<String, Object>> commodityList) {
		this.commodityList = commodityList;
	}

	public Integer getActivityType() {
		return activityType;
	}

	public void setActivityType(Integer activityType) {
		this.activityType = activityType;
	}

	public String getImg() {
		return img;
	}

	public void setImg(String img) {
		this.img = img;
	}

	public String getLink() {
		return link;
	}

	public void setLink(String link) {
		this.link = link;
	}
}
