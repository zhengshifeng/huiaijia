public class HajSaleDeptConstant {

	public final static int OTHER = 0;
	public final static int PRODUCE = 1;
	public final static int QUALITY = 2;
	public final static int DELIVERY = 3;
	public final static int PURCHASE = 4;
	public final static int STORAGE = 5;

	public static String getSaleDept(int dept) {
		String realValue;
		switch (dept) {
			case PRODUCE:
				realValue = "生产部";
				break;
			case QUALITY:
				realValue = "质检部";
				break;
			case DELIVERY:
				realValue = "配送部";
				break;
			case PURCHASE:
				realValue = "采购部";
				break;
			case STORAGE:
				realValue = "仓储部";
				break;
			default:
				realValue = "无法判断";
		}
		return realValue;
	}

}
